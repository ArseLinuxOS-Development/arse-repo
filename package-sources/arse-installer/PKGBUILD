# Maintainer: ArseLinuxOS-Team

pkgname=arse-installer
pkgver=1.0
pkgrel=3
pkgdesc='ArseLinuxOS installer'
arch=('any')
license=('GPL3')
depends=(libnotify)

source=('installer' )

sha512sums=('7226162ddcf9b304c0daa5de6b1716508d1ab36823a53c2c216efcdcc82adcc21f53b33ce903efb60546c566caddc488fc9635539fbeb59b20b7c6582a42e77b')

package() {
  local installer=$pkgdir/usr/bin/

  install -Dm777 installer      $installer/installer
}
