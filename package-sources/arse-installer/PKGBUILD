# Maintainer: ArseLinuxOS-Team

pkgname=arse-installer
pkgver=1.0
pkgrel=4
pkgdesc='ArseLinuxOS installer'
arch=('any')
license=('GPL3')
depends=(libnotify)

source=('installer' )

sha512sums=('fc1c7caf0c87807f90bdeb22c3a84d8977e1754b306d6891291e4977ab823a50f79ec24bb95cb0c31caf42a69aa89e0eb97eea6ef245be86998cc78569f2a539')

package() {
  local installer=$pkgdir/usr/bin/

  install -Dm777 installer      $installer/installer
}
