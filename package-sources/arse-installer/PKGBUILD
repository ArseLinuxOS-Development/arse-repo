# Maintainer: ArseLinuxOS-Team

pkgname=arse-installer
pkgver=1.0
pkgrel=5
pkgdesc='ArseLinuxOS installer'
arch=('any')
license=('GPL3')
depends=(libnotify)

source=('installer' )

sha512sums=('43e1c05931a8c179408c8f72a548f85bd74bafff367b50267e0df23a923870410c41accbfefa1ca5694207825c68c1e967bf34878e16f7995b04faa7d31a5834')

package() {
  local installer=$pkgdir/usr/bin/

  install -Dm777 installer      $installer/installer
}
